{
  "name": "russable",
  "version": "1.0.0",
  "description": "Russable - The billionaire's choice for AI-powered web development. This guy fucks... at coding!",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=backend",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "prisma:generate": "npm run prisma:generate --workspace=backend",
    "prisma:migrate:dev": "npm run prisma:migrate:dev --workspace=backend --name init",
    "prisma:migrate:deploy": "npm run prisma:migrate:deploy --workspace=backend",
    "prisma:db:push": "npm run prisma:db:push --workspace=backend",
    "prisma:studio": "npm run prisma:studio --workspace=backend",
    "setup:dev": "npm run prisma:generate && npm run prisma:db:push"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "russable",
    "russ-hanneman",
    "ai",
    "website-generator",
    "three-comma-club",
    "silicon-valley",
    "billionaire-tech"
  ],
  "author": "Russ Hanneman (Three Comma Club Member)",
  "license": "MIT"
}
